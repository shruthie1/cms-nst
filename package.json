{
  "name": "cms-nst",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.0.0 <21.0.0"
  },
  "scripts": {
    "build": "npm update commonService && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_OPTIONS=--expose-gc nest start",
    "start:prod": "cross-env NODE_OPTIONS=--expose-gc node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack-dev": "webpack --watch --mode development",
    "webpack": "webpack --mode  development",
    "compile": "npm run webpack",
    "compile-dev": "npm run webpack-dev",
    "build:webpack": "npm i && npm run webpack",
    "upload-cloudinary": "node upload-build.js",
    "start:node": "node  --max-old-space-size=8192 ./out/index.js"
  },
  "dependencies": {
    "ams-ssk": "^1.0.29",
    "common-tg-service": "^1.3.1",
    "node-schedule-tz": "^1.2.1-4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "cross-env": "^10.0.0",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "optionalDependencies": {
    "@aws-sdk/credential-providers": "^3.888.0",
    "@mongodb-js/zstd": "^2.0.1",
    "gcp-metadata": "^7.0.1",
    "kerberos": "^2.2.2",
    "mongodb-client-encryption": "^6.5.0",
    "snappy": "^7.3.3"
  }
}
